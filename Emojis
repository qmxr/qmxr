Pew = {
405922557,
}

Sleepy = {
474478606,
2356433704,
3123985966,
}

function Emojis()
	for _,Player in pairs(game:GetService("Players"):GetChildren()) do
		if table.find(Prem, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == "Players" then
					Player.Character:FindFirstChildWhichIsA("Humanoid").DisplayName = ("[‚≠ê]" .. Player.DisplayName)
				end
			end
		elseif
			table.find(Pew, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == "Players" then
					Player.Character:FindFirstChildWhichIsA("Humanoid").DisplayName = ("[üíé]" .. Player.DisplayName)
				end
			end
		elseif
			table.find(Sleepy, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == "Players" then
					Player.Character:FindFirstChildWhichIsA("Humanoid").DisplayName = ("[üëë]" .. Player.DisplayName)
				end
			end
		if
		  Player.Character then
			if Player.Character.Parent.Name == "Players" then
				if not Player.Character.LeftHand:FindFirstChild("OriginalSize") then
					Player.Character:FindFirstChildWhichIsA("Humanoid").DisplayName = ("[üò≥]" .. Player.DisplayName)
				end
			end
		end
	end
  end
end
local success,err = pcall(Emojis)
return Emojis
